					}
				],
				"typeProperties": {
					"format": {
						"type": "ParquetFormat"
					},
					"fileName": {
						"value": "@concat('LOAN_RECON_DATA_FOR_DATALAKE','.parquet')",
						"type": "Expression"
					},
					"folderPath": {
						"value": "landing-zone/change-captured-data-oracle/loan_recon_data_for_datalake/full_load",
						"type": "Expression"
					}
				}
			}
		},
