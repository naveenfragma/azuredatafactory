{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "bajaj-poc-df"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/BlobToEdwPipeline')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy Data To EDW",
						"type": "Copy",
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"typeProperties": {
							"source": {
								"type": "BlobSource",
								"recursive": true
							},
							"sink": {
								"type": "SqlDWSink",
								"allowPolyBase": false,
								"writeBatchSize": 10000
							},
							"enableStaging": false,
							"dataIntegrationUnits": 0
						},
						"inputs": [
							{
								"referenceName": "AzureBlobPoc",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "AzureSqlDWPoc",
								"type": "DatasetReference"
							}
						]
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/AzureBlobPoc')]",
				"[concat(variables('factoryId'), '/datasets/AzureSqlDWPoc')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/BlobToOracleRecon')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "ReconActivity",
						"type": "Copy",
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"typeProperties": {
							"source": {
								"type": "BlobSource",
								"recursive": true
							},
							"sink": {
								"type": "OracleSink",
								"writeBatchSize": 10000
							},
							"enableStaging": false,
							"dataIntegrationUnits": 0,
							"translator": {
								"type": "TabularTranslator",
								"columnMappings": {
									"APPL_ID": "APPL_ID",
									"EMI": "EMI",
									"CUSTOMER_ID": "CUSTOMER_ID",
									"INSTALLMENT_OVERDUE": "INSTALLMENT_OVERDUE",
									"AMOUNT_OUTSTANDING": "AMOUNT_OUTSTANDING",
									"EMI_PAID": "EMI_PAID",
									"PRINCIPAL_OVERDUE": "PRINCIPAL_OVERDUE",
									"PRINCIPAL_OUTSTANDING": "PRINCIPAL_OUTSTANDING",
									"INTEREST_OVERDUE": "INTEREST_OVERDUE",
									"INTEREST_OUTSTANDING": "INTEREST_OUTSTANDING",
									"BUCKET": "BUCKET",
									"PENALTY_PAID": "PENALTY_PAID",
									"PENALTY_DUE": "PENALTY_DUE",
									"BCC_PAID": "BCC_PAID",
									"BCC_DUE": "BCC_DUE",
									"OTHER_PAID": "OTHER_PAID",
									"OTHER_DUE": "OTHER_DUE",
									"PRINCIPLE_AMOUNT_PAID": "PRINCIPLE_AMOUNT_PAID",
									"INTEREST_PAID": "INTEREST_PAID",
									"BALANCE_TENURE": "BALANCE_TENURE",
									"LAST_EMI_DATE": "LAST_EMI_DATE",
									"NEXT_EMI_DATE": "NEXT_EMI_DATE"
								}
							}
						},
						"inputs": [
							{
								"referenceName": "ReconBlob",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "OracleTable",
								"type": "DatasetReference"
							}
						]
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/ReconBlob')]",
				"[concat(variables('factoryId'), '/datasets/OracleTable')]"
			]
		},
