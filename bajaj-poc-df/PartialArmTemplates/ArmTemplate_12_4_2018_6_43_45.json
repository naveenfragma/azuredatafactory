		{
			"name": "[concat(parameters('factoryName'), '/AzureBlob')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureBlobLandingZone",
					"type": "LinkedServiceReference"
				},
				"type": "AzureBlob",
				"structure": [
					{
						"name": "APPL_ID",
						"type": "Int64"
					}
				],
				"typeProperties": {
					"format": {
						"type": "ParquetFormat"
					},
					"fileName": {
						"value": "@concat('LOAN_RECON_DATA_FOR_DATALAKE','.parquet')",
						"type": "Expression"
					},
					"folderPath": {
						"value": "landing-zone/change-captured-data/loan_recon_data_for_datalake/full_load",
						"type": "Expression"
					}
				}
			}
		},
